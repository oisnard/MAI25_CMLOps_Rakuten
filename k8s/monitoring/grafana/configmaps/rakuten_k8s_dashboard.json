{
  "dashboard": {
    "id": null,
    "uid": null,
    "title": "Rakuten - K8s Monitoring",
    "tags": [
      "kubernetes",
      "rakuten",
      "monitoring"
    ],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "type": "timeseries",
        "title": "Rakuten API - Nombre de Pods",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "id": 1001,
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "count(kube_pod_info{namespace=\"apps\", pod=~\"rakuten-api.*\"})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 2
                },
                {
                  "color": "red",
                  "value": 3
                }
              ]
            },
            "unit": "none"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single",
            "sort": "none"
          }
        }
      }
    ]
  },
  "overwrite": true
}